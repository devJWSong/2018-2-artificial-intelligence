parcel([75,50,75], [1,2,0]).
parcel([100,75,75], [0,2,1]).
parcel([50,75,75], [1,2,0]).
parcel([50,50,100], [2,0,1]).
parcel([50,50,75], [2,1,0]).
parcel([50,100,50], [2,0,1]).
parcel([50,75,50], [2,1,0]).
parcel([100,50,50], [2,0,1]).
parcel([75,50,50], [2,1,0]).
parcel([100,75,50], [1,1,1]).
parcel([100,50,75], [1,1,1]).
parcel([75,100,50], [1,1,1]).
parcel([75,50,100], [1,1,1]).
parcel([50,100,75], [1,1,1]).
parcel([50,75,100], [1,1,1]).


moves(parcel([100,100,100], [0,0,3]), parcel([75,75,75], [0,3,0]), parcel([50,50,50], [3,0,0]), parcel([100,100,75], [0,1,2]), parcel([100,100,50], [1,0,2]), parcel([100,75,100], [0,1,2]), parcel([100,50,100], [1,0,2]), parcel([75,100,100], [0,1,2]), parcel([50,100,100], [1,0,2]), parcel([75,75,100], [0,2,1]), parcel([75,75,50], [1,2,0]), parcel([75,100,75], [0,2,1]), parcel([75,50,75], [1,2,0]), parcel([100,75,75], [0,2,1]), parcel([50,75,75], [1,2,0]), parcel([50,50,100], [2,0,1]), parcel([50,50,75], [2,1,0]), parcel([50,100,50], [2,0,1]), parcel([50,75,50], [2,1,0]), parcel([100,50,50], [2,0,1]), parcel([75,50,50], [2,1,0]), parcel([100,75,50], [1,1,1]), parcel([100,50,75], [1,1,1]), parcel([75,100,50], [1,1,1]), parcel([75,50,100], [1,1,1]), parcel([50,100,75], [1,1,1]), parcel([50,75,100], [1,1,1])).

search(Current, Moves, Goal):-
    load(Current, Moves, Current),
    search(Current, Moves, Goal).
