list_add([], [], []).
list_add([H1|T1], [H2|T2], [H3|T3]):-
    H3 is H1 + H2,
    list_add(T1, T2, T3).

list_sub([], [], []).
list_sub([H1|T1], [H2|T2], [H3|T3]):-
    H3 is H1 - H2,
    list_sub(T1, T2, T3).

load([H1|T1], [H2|T2], [H3|T3]):-
    list_add(H1, H2, H3),
    list_sub(T1, T2, T3).

list_small([], []).
list_small([H1|T1], [H2|T2]):-
    H1 < H2,
    list_small(T1, T2).

list_eqsmall([], []).
list_eqsmall([H1|T1], [H2|T2]):-
    H1 =< H2,
    list_small(T1, T2).

list_equal([], []).
list_equal([H1|T1], [H2|T2]):-
    H1 =:= H2,
    list_equal(T1, T2).

load_able([H1|T1], L):-
    list_small(H1, [1500,1200,800]),
    list_eqsmall(L, T1).

parcel([100,100,100], [0,0,3]).
parcel([75,75,75], [0,3,0]).
parcel([50,50,50], [3,0,0]).
parcel([100,100,75], [0,1,2]).
